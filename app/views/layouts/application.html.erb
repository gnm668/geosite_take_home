<!DOCTYPE html>
<html>
  <head>
    <title>Geosite</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class='date'>
      <%= %x(date) %>
    </div>
    <%= yield %>
  </body>

    <div class='cpu'>
      <%= %x(sysctl -a | grep machdep.cpu) %>
    </div>
</html>
